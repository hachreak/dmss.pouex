<?xml version="1.0" encoding="ASCII"?>
<pouexmm:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pouexmm="http://pouexmm/1.0"
    name="game03">
  <pouex name="MyPou">
    <actions
        xsi:type="pouexmm:PouexAction"
        name="change sleep"
        enable="true">
      <influences
          feature="//@pouex/@features.0"
          value="3.0"/>
    </actions>
    <states
        name="TestStato">
      <activationConditions
          xsi:type="pouexmm:StateActivationCondition"
          name="TestStato2"
          state="//@pouex/@states.0"/>
    </states>
    <states
        name="TestStato3">
      <activationConditions
          xsi:type="pouexmm:MinThresholdActivationCondition"
          name="fuu"
          threshold="2.0"
          feature="//@pouex/@features.0"/>
    </states>
    <features
        xsi:type="pouexmm:Emotional"
        default="5.0"
        maximum="10.0"
        name="sleep"
        MeasurementUnit=""/>
    <bodyparts
        name="Face"
        image="face.jpg">
      <subparts
          name="eye sx"
          image="eye.jpg"/>
      <subparts
          name="eye dx"
          image="eye.jpg"/>
    </bodyparts>
  </pouex>
</pouexmm:Game>
