<?xml version="1.0" encoding="ASCII"?>
<pouexmm:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pouexmm="http://pouexmm/1.0">
  <pouex name="MyPou">
    <actions
        xsi:type="pouexmm:PouexAction"
        name="change sleep">
      <influences
          feature="//@pouex/@features.0"/>
    </actions>
    <states
        name="TestStato">
      <activationConditions
          xsi:type="pouexmm:StateActivationCondition"
          features="//@pouex/@features.0"
          name="TestStato2"
          state="//@pouex/@states.0"/>
    </states>
    <states
        name="TestStato3"/>
    <features
        xsi:type="pouexmm:Emotional"
        default="5.0"
        maximum="10.0"
        name="sleep"/>
    <bodyparts
        name="Face"
        image="face.jpg">
      <subparts
          name="eye sx"
          image="eye.jpg"/>
      <subparts
          name="eye dx"
          image="eye.jpg">
        <actions
            xsi:type="pouexmm:PouexAction"
            name="close">
          <influences
              feature="//@pouex/@features.0"
              value="0.1"/>
        </actions>
      </subparts>
    </bodyparts>
  </pouex>
</pouexmm:Game>
