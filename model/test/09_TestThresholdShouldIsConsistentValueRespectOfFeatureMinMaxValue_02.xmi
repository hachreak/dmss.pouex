<?xml version="1.0" encoding="ASCII"?>
<pouexmm:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pouexmm="http://pouexmm/1.0"
    name="test">
  <pouex name="Pluto">
    <actions
        xsi:type="pouexmm:PouexAction"
        name="test"
        enable="true">
      <influences
          feature="//@pouex/@features.0"
          value="1.0"/>
      <influences
          feature="//@pouex/@features.1"
          operator="MULTIPLICATION"
          value="3.0"/>
    </actions>
    <states
        name="Happy">
      <activationConditions
          xsi:type="pouexmm:MinThresholdActivationCondition"
          name="if sufficiently thin"
          threshold="10000.0"
          feature="//@pouex/@features.0"/>
    </states>
    <features
        xsi:type="pouexmm:Physic"
        default="20.0"
        minimum="10.0"
        maximum="100.0"
        name="weight"
        MeasurementUnit="kg"/>
    <features
        xsi:type="pouexmm:Emotional"
        default="4.0"
        maximum="10.0"
        name="Happiness"/>
    <bodyparts
        name="Face"/>
  </pouex>
</pouexmm:Game>
