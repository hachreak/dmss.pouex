<?xml version="1.0" encoding="ASCII"?>
<pouexmm:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pouexmm="http://pouexmm/1.0"
    xsi:schemaLocation="http://pouexmm/1.0 ../../metamodel/PouexMM.ecore">
  <pouex name="Pluto">
    <features
        xsi:type="pouexmm:Physic"
        default="1.0"
        minimum="5.0"
        maximum="10.0"
        name="weight"/>
    <features
        xsi:type="pouexmm:Emotional"
        default="4.0"
        maximum="10.0"
        name="Happiness"/>
    <actions
        xsi:type="pouexmm:PouexAction"
        name="test">
      <influences
          feature="//@pouex/@features.0"
          value="1.0"/>
      <influences
          feature="//@pouex/@features.1"
          operator="MULTIPLICATION"
          value="3.0"/>
    </actions>
    <bodyparts
        name="Face"/>
  </pouex>
</pouexmm:Game>
