<?xml version="1.0" encoding="ASCII"?>
<pouexmm:Game
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:pouexmm="http://pouexmm/1.0"
    name="index">
  <pouex name="Pippo">
    <actions
        xsi:type="pouexmm:PouexAction"
        name="Clean"
        enable="true">
      <influences
          feature="//@pouex/@features.2"
          operator="SUBTRACTION"
          value="1.0"/>
    </actions>
    <actions
        xsi:type="pouexmm:PouexAction"
        name="Eat"
        enable="true">
      <influences
          feature="//@pouex/@features.0"
          value="0.1"/>
    </actions>
    <actions
        xsi:type="pouexmm:PouexAction"
        name="Play"
        enable="true">
      <influences
          feature="//@pouex/@features.4"
          value="0.2"/>
      <influences
          feature="//@pouex/@features.1"
          value="0.2"/>
      <influences
          feature="//@pouex/@features.3"
          value="0.1"/>
      <influences
          feature="//@pouex/@features.0"
          value="0.05"/>
    </actions>
    <actions
        xsi:type="pouexmm:TimeEvent"
        name="Time passes"
        enable="true">
      <influences
          feature="//@pouex/@features.2"
          value="0.1"/>
      <influences
          feature="//@pouex/@features.1"
          operator="SUBTRACTION"
          value="0.1"/>
      <influences
          feature="//@pouex/@features.0"
          operator="SUBTRACTION"
          value="0.05"/>
      <influences
          feature="//@pouex/@features.5"
          value="0.1"/>
    </actions>
    <actions
        xsi:type="pouexmm:RepeatedAction"
        name="Yawn"
        enable="true"
        eachMillisec="60000">
      <influences
          feature="//@pouex/@features.4"
          value="0.1"/>
    </actions>
    <states
        name="Hungry">
      <activationConditions
          xsi:type="pouexmm:MinThresholdActivationCondition"
          name="when is hungry"
          threshold="8.0"
          feature="//@pouex/@features.3"/>
    </states>
    <states
        name="Dead">
      <activationConditions
          xsi:type="pouexmm:MinThresholdActivationCondition"
          name="if not happy"
          feature="//@pouex/@features.1"/>
      <activationConditions
          xsi:type="pouexmm:MaxThresholdActivationCondition"
          name="if to much fat"
          threshold="100.0"
          feature="//@pouex/@features.0"/>
      <activationConditions
          xsi:type="pouexmm:MaxThresholdActivationCondition"
          name="if to much dirty"
          threshold="9.0"
          feature="//@pouex/@features.2"/>
      <activationConditions
          xsi:type="pouexmm:MinThresholdActivationCondition"
          name="if to much old"
          threshold="100.0"
          feature="//@pouex/@features.5"/>
    </states>
    <states
        name="Dirty">
      <activationConditions
          xsi:type="pouexmm:MaxThresholdActivationCondition"
          name="if to much dirty"
          threshold="7.0"
          feature="//@pouex/@features.2"/>
    </states>
    <states
        name="Tired">
      <activationConditions
          xsi:type="pouexmm:MinThresholdActivationCondition"
          name="when is tired"
          threshold="8.0"
          feature="//@pouex/@features.4"/>
    </states>
    <features
        xsi:type="pouexmm:Physic"
        default="5.0"
        minimum="1.0"
        maximum="100.0"
        name="weight"/>
    <features
        xsi:type="pouexmm:Emotional"
        default="5.0"
        maximum="10.0"
        name="happiness"/>
    <features
        xsi:type="pouexmm:Physic"
        default="3.0"
        maximum="10.0"
        name="Dirty"/>
    <features
        xsi:type="pouexmm:Emotional"
        default="5.0"
        maximum="10.0"
        name="Hungry"/>
    <features
        xsi:type="pouexmm:Physic"
        default="3.0"
        maximum="10.0"
        name="Tiredness"/>
    <features
        xsi:type="pouexmm:Physic"
        maximum="100.0"
        name="Age"/>
    <bodyparts
        name="Eye SX"/>
    <bodyparts
        name="Eye DX"/>
  </pouex>
</pouexmm:Game>
